{{#each this}}
<li class="photo-card">
    <a data-fslightbox href="{{largeImageURL}}" class="image-link">
        <img src="{{webformatURL}}" alt={{tags}} class="card-image js-image" data-source="{{largeImageURL}}" />
    </a>
    <ul class="stats">
        <li class="stats-item">
            <i class="material-icons">thumb_up</i>
            {{likes}}
        </li>
        <li class="stats-item">
            <i class="material-icons">visibility</i>
            {{views}}
        </li>
        <li class="stats-item">
            <i class="material-icons">comment</i>
            {{comments}}
        </li>
        <li class="stats-item">
            <i class="material-icons">cloud_download</i>
            {{downloads}}
        </li>
    </ul>
</li>
{{/each}}